<!doctype html>
<html>
  {{> head}}
  <body>
    <main>
      <section id="navigation">
          {{> navbar}} 
      </section>
      <section id="form-creation-module">
        <div id="form-information-text">
          <h2>Let's Get Started</h2>
          <p>Please enter the following information:</p>
        </div>
        
        <form method="POST" action="/dashboard"  >
          {{> personal}}  
          {{> experience}}   
          {{> education}}  
          {{> skills}}
        <br />
        <div class="btn-container">
            <input type="submit" value="Submit" class="btn-small" />
        </div>
      </form>
      </section>
      <section>
        <h2>Resume</h2>
        <ul>
          <fieldset>
            {{> resume}}
          </fieldset>
        </ul>
      </section>
    </main>
  </body>
</html>
    
